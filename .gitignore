# Git ignore configuration for Hablaqui project
# This file specifies intentionally untracked files that Git should ignore
# Files and directories that should not be committed to version control

# Dependencies - Node.js package directories
node_modules/        # Node.js dependencies
.pnp/               # Plug'n'Play dependencies
.pnp.js             # Plug'n'Play configuration

# Testing - Test coverage and output directories
coverage/           # Test coverage reports
.nyc_output/        # NYC (Istanbul) test coverage output

# Production - Build output directories
build/              # Compiled code
dist/               # Distribution files
out/                # Output directory

# Environment variables - Configuration files with sensitive data
.env                # Main environment file
.env.local          # Local environment overrides
.env.development.local  # Development environment
.env.test.local     # Test environment
.env.production.local   # Production environment

# Debug logs - Log files
npm-debug.log*      # npm debug logs
yarn-debug.log*     # yarn debug logs
yarn-error.log*     # yarn error logs

# IDE and editor files - Editor-specific files
.idea/              # JetBrains IDE files
.vscode/            # Visual Studio Code files
*.swp               # Vim swap files
*.swo               # Vim swap files
.DS_Store           # macOS system files
Thumbs.db           # Windows thumbnail cache

# Mobile specific - Flutter/Dart related files
movil/.dart_tool/   # Dart tool directory
movil/.flutter-plugins  # Flutter plugins
movil/.flutter-plugins-dependencies  # Flutter plugin dependencies
movil/.packages     # Dart package dependencies
movil/.pub-cache/   # Pub cache directory
movil/.pub/         # Pub package directory
movil/build/        # Flutter build output
movil/ios/Pods/     # iOS CocoaPods
movil/android/.gradle/  # Android Gradle files
movil/android/app/build/  # Android build output
movil/android/local.properties  # Android local properties

# API specific - Backend related files
api/node_modules/   # API dependencies
api/dist/           # API build output
api/.env            # API environment file
api/coverage/       # API test coverage
api/.nyc_output/    # API test coverage output

# Landing specific - Frontend related files
landing/node_modules/  # Landing page dependencies
landing/dist/       # Landing page build output
landing/.env        # Landing page environment
landing/.nuxt/      # Nuxt.js build directory
landing/.output/    # Nuxt.js output directory

# Temporary files - Temporary and cache files
*.log              # Log files
*.tmp              # Temporary files
*.temp             # Temporary files
.cache/            # Cache directory
.temp/             # Temporary directory
.tmp/              # Temporary directory

# System files - Operating system files
.DS_Store          # macOS system files
.DS_Store?         # macOS system files
._*                # macOS resource forks
.Spotlight-V100    # macOS Spotlight index
.Trashes           # macOS trash info
ehthumbs.db        # Windows thumbnail cache
Thumbs.db          # Windows thumbnail cache

# Lock files - Package manager lock files
api/package-lock.json    # npm lock file for API
api/yarn.lock           # yarn lock file for API
landing/package-lock.json  # npm lock file for landing
landing/yarn.lock        # yarn lock file for landing
front/package-lock.json  # npm lock file for frontend
front/yarn.lock         # yarn lock file for frontend

# Logs - Application logs
logs               # Log directory
*.log              # Log files
npm-debug.log*     # npm debug logs
yarn-debug.log*    # yarn debug logs
yarn-error.log*    # yarn error logs
lerna-debug.log*   # Lerna debug logs

# Diagnostic reports - Node.js diagnostic files
report.[0-9]*.[0-9]*.[0-9]*.[0-9]*.json  # Node.js diagnostic reports

# Runtime data - Runtime files
pids               # Process ID files
*.pid              # Process ID files
*.seed             # Seed files
*.pid.lock         # Process ID lock files

# Coverage directories - Test coverage
lib-cov            # Library coverage
coverage           # Coverage directory
*.lcov             # LCOV coverage files
.nyc_output        # NYC test coverage

# Build tools - Build tool directories
.grunt             # Grunt build tool
bower_components   # Bower components
.lock-wscript      # Node-waf configuration
build/Release      # Compiled binary addons

# Dependency directories - Package manager directories
jspm_packages      # JSPM packages

# TypeScript - TypeScript related files
typings/           # TypeScript declaration files
*.tsbuildinfo      # TypeScript build info

# Cache directories - Various cache directories
.npm               # npm cache
.eslintcache       # ESLint cache
.rpt2_cache/       # Microbundle cache
.rts2_cache_cjs/   # Microbundle cache
.rts2_cache_es/    # Microbundle cache
.rts2_cache_umd/   # Microbundle cache

# REPL history - Node.js REPL history
.node_repl_history # Node.js REPL history

# Package files - Package manager files
*.tgz              # npm package files
.yarn-integrity    # Yarn integrity file

# Environment files - Environment configuration
.env.test          # Test environment
.env.example       # Example environment

# Build tool caches - Build tool cache directories
.cache             # Parcel bundler cache
.next              # Next.js build output
.nuxt              # Nuxt.js build output
dist               # Distribution directory
.cache/            # Gatsby cache
.vuepress/dist     # VuePress build output
.serverless/       # Serverless directories
.fusebox/          # FuseBox cache
.dynamodb/         # DynamoDB local files
.tern-port         # TernJS port file

# IDE specific - IDE configuration files
api/.idea          # API IDE files
front/.idea        # Frontend IDE files
landing/.idea      # Landing page IDE files
.vscode            # VSCode configuration