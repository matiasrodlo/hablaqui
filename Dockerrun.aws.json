{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "api-restful",
        "image": "782546527822.dkr.ecr.us-east-2.amazonaws.com/hablaqui-staging-api:latest",
        "hostname": "node",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 3000,
            "containerPort": 3000
          }
        ],
        "memory": 1500
      },
      {
			"name": "landing-ui",
			"image": "782546527822.dkr.ecr.us-east-2.amazonaws.com/hablaqui-staging-landing:latest",
			"hostname": "vue",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 8080,
					"containerPort": 8080
				},
				{
					"hostPort": 9000,
					"containerPort": 9000
				}
			],
			"memory": 1500
		}
    ]
}